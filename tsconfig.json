{
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "Node",
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ]
  },
  "files": [
    "web_src/index.ts"
  ],
  "include": [
    "./web_src/",
  ],
  "exclude": [
    "./node_modules/"
  ]
}